{
  "extends": [
    "airbnb",
     "prettier"
  ],
  "env": {
    "browser": true,
    "commonjs": true,
    "es6": true,
    "jest": true,
    "node": true
  },
  "rules": {
    "jsx-a11y/href-no-hash": ["off"],
    "react/jsx-filename-extension": ["warn", { "extensions": [".js", ".jsx"] }],
    "no-unused-vars":"warn",
    "no-underscore-dangle": "off",
    "no-console":"off"
  }
}
